<?xml version="1.0" encoding="utf-8"?>
<resources>
    <declare-styleable name="VerCodeEditText">
        <attr name="vcCount" format="integer" />
        <attr name="vcMaxLength" format="integer" />
        <!---->
        <attr name="vcNormalBackground" format="reference" />
        <attr name="vcFocusedBackground" format="reference" />
        <!---->
        <attr name="vcWidth" format="dimension" />
        <attr name="vcHeight" format="dimension" />
        <attr name="vcMinWidth" format="dimension" />
        <attr name="vcMinHeight" format="dimension" />
        <!---->
        <attr name="vcMargin" format="dimension" />
        <attr name="vcMarginLeft" format="dimension" />
        <attr name="vcMarginTop" format="dimension" />
        <attr name="vcMarginRight" format="dimension" />
        <attr name="vcMarginBottom" format="dimension" />
        <!---->
        <attr name="vcPadding" format="dimension" />
        <attr name="vcPaddingLeft" format="dimension" />
        <attr name="vcPaddingTop" format="dimension" />
        <attr name="vcPaddingRight" format="dimension" />
        <attr name="vcPaddingBottom" format="dimension" />
        <!---->
        <attr name="vcTextSize" format="dimension" />
        <attr name="vcTextColor" format="color" />
        <attr name="vcTextCursorDrawable" format="reference" />
        <attr name="vcGravity" format="flags">
            <flag name="top" value="0x30" />
            <flag name="bottom" value="0x50" />
            <flag name="left" value="0x03" />
            <flag name="right" value="0x05" />
            <flag name="center_vertical" value="0x10" />
            <flag name="fill_vertical" value="0x70" />
            <flag name="center_horizontal" value="0x01" />
            <flag name="fill_horizontal" value="0x07" />
            <flag name="center" value="0x11" />
            <flag name="fill" value="0x77" />
            <flag name="clip_vertical" value="0x80" />
            <flag name="clip_horizontal" value="0x08" />
            <flag name="start" value="0x00800003" />
            <flag name="end" value="0x00800005" />
        </attr>
    </declare-styleable>
</resources>